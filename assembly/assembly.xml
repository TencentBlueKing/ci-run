<assembly>
    <id>bin</id>
    <formats>
        <format>zip</format>
    </formats>
    <fileSets>
        <!-- 从目标目录拷贝文件去压缩 -->
        <fileSet>
            <directory>target</directory>
            <includes>
                <include>run-jar-with-dependencies.jar</include>
                <include>../task.json</include>
            </includes>
            <outputDirectory>/</outputDirectory>
        </fileSet>
        <fileSet>
            <directory></directory>
            <includes>
                <include>task.json</include>
            </includes>
            <outputDirectory>/</outputDirectory>
        </fileSet>
        <fileSet>
            <directory></directory>
            <includes>
                <include>README_EN.md</include>
            </includes>
            <outputDirectory>/file</outputDirectory>
        </fileSet>
        <fileSet>
            <directory>img</directory>
            <includes>
                <include>command.png</include>
                <include>logo.png</include>
            </includes>
            <outputDirectory>/file</outputDirectory>
        </fileSet>
    </fileSets>
</assembly>